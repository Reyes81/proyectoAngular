import { Club } from "./Club";
import { Persona } from "./Persona";

export class Entrenador extends Persona {
    
    id: number;
    clubes: Club[];
    
    constructor(){
        super();
        this.id = -1;
        this.nombre = '';
        this.apellido = '';
        this.edad = 0;

        this.user = '';
        this.passwd = '';

        this.clubes = [];
        }
    }